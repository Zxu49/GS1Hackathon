<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/empty" />

    <Button
        android:id="@+id/confirm"
        android:layout_width="127dp"
        android:layout_height="66dp"
        android:text="confirm"
        tools:layout_editor_absoluteX="142dp"
        tools:layout_editor_absoluteY="598dp" />

    <TextView
        android:id="@+id/infomationreview"
        android:layout_width="302dp"
        android:layout_height="226dp"
        android:background="#EAEEF1"
        android:shadowColor="#00BCD4"
        android:text="InfomationReview"
        android:textColor="#0C0B0B"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="347dp" />

    <CheckedTextView
        android:id="@+id/checkedTextView"
        android:layout_width="300dp"
        android:layout_height="129dp"
        android:background="#EDF5F8"
        android:text="CheckedTextView"
        android:textColorHighlight="#00DB9D9D"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="193dp" />

</android.support.constraint.ConstraintLayout>